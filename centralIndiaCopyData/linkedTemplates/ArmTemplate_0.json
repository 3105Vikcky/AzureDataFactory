			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/flowlet1')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Flowlet",
				"typeProperties": {
					"sources": [],
					"sinks": [],
					"transformations": [
						{
							"name": "aggregate1"
						},
						{
							"name": "select1"
						},
						{
							"name": "input1"
						},
						{
							"name": "output1"
						}
					],
					"scriptLines": [
						"input(output(",
						"          rollno as string,",
						"          Full_name as string,",
						"          marks as string,",
						"          grade as string,",
						"          city as string,",
						"          DOB as string",
						"     ),",
						"     order: 0,",
						"     allowSchemaDrift: true) ~> input1",
						"input1 aggregate(groupBy(Full_name,",
						"          marks,",
						"          grade,",
						"          city,",
						"          DOB),",
						"     count = count()) ~> aggregate1",
						"aggregate1 select(mapColumn(",
						"          Full_name,",
						"          marks,",
						"          grade,",
						"          city,",
						"          DOB",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> select1",
						"select1 output() ~> output1"
					]
				}
			},
			"dependsOn": []
		},
